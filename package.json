    "url": "git+https://github.com/bhaireshm/extract-languages.git"
  },
  "author": "Bhairesh M <bhaireshm@arithaconsulting.com>",
  "license": "ISC",
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/yargs": "^17.0.33"
  }
}
