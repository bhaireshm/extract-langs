{
  "name": "@bhaireshm/extract-languages",
  "version": "1.1.0",
  "description": "A Package and CLI tool to extract languages from files",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "el": "dist/src/cli.js",
    "extract-languages": "dist/src/cli.js"
  },
  "scripts": {
